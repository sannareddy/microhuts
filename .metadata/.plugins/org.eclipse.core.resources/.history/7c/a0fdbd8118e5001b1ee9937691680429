package in.codehut.paymentservice.controller;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.bind.annotation.RestController;

import in.codehut.paymentservice.PaymentService;

@RestController
@RequestMapping("/paymentService")
public class PaymentServiceController {
	
	@GetMapping("")
	@ResponseBody
	public double getPaymentId() {
		PaymentService paymentservice=new PaymentService(1089888);
		return paymentservice.getPaymentId();
		
	}

}
